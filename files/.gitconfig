[user]
	name = Mark Turnley
  email = mark.turnley@envato.com
[color]
	diff = auto
	status = auto
	branch = auto
[core]
	excludesfile = /Users/mturnley/.gitignore
  editor = /usr/bin/vim
[github]
	user = rabidcarrot
[push]
 default = current
[alias]
  track = !sh -c 'git branch --track "$0" "origin/$0" && git checkout "$0"'
  ignored = "!git ls-files --others --exclude-standard"
  delete = !sh -c 'git branch -D "$0" && git push origin ":$0"'
  find-branch = !sh -c 'git branch -a | grep $1'
  st = status
  cav = commit -a -v
  co = checkout
  wtf = !git-wtf
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[apply]
  whitespace = nowarn
[branch]
  autosetupmerge = true
